@article{BowersPrato2019,
	author = {Bowers, Anne and Prato, Matteo},
	date-modified = {2022-02-01 15:33:40 +0100},
	doi = {10.1287/orsc.2019.1316},
	journal = {Organization Science},
	keywords = {Quantifizierung,Ranking,Rankings und M{\"a}rkte,Statussymbol},
	shortjournal = {Organization Science},
	shorttitle = {The {{Role}} of {{Third}}-{{Party Rankings}} in {{Status Dynamics}}},
	title = {The {{Role}} of {{Third}}-{{Party Rankings}} in {{Status Dynamics}}: {{How Does}} the {{Stability}} of {{Rankings Induce Status Changes}}?},
	url = {https://pubsonline.informs.org/doi/full/10.1287/orsc.2019.1316},
	urlyear = {2019},
	year = {2019},
	abstract = {Most explanations of status dynamics rely on market actor behavior or affiliation to other actors as the primary drivers of change. Yet status is increasingly mediated by third-party intermediaries, which impart status through their ordering of actors. Prior literature suggests that these rankers can affect status orders via changes in the underlying ranking methodology but offers little insight as to whether such changes reflect existing field beliefs or are self-interested. We advance a theory of ranker self-interest, whereby rankers adopt specific behavior to maintain audience attention and increase their chance for survival. We hypothesize that, by threatening audience attention, temporal stability in rankings (an endogenous property of many status systems) induces rankers to self-generate changes in the ranking. We examine the role of stability of rankings in promoting structural changes by rankers using Institutional Investor magazine's All-America Research Team (all-stars), a widely studied and eminently impactful ranking of equity analysts.},
	bdsk-url-1 = {https://pubsonline.informs.org/doi/full/10.1287/orsc.2019.1316},
	bdsk-url-2 = {https://doi.org/10.1287/orsc.2019.1316}}

@article{Bowker1993,
