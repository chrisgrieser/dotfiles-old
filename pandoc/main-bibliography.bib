	keywords = {Wirtschaft,Arbeitsmarkt,Monopol},
	langid = {german},
	number = {4},
	pages = {597--622},
	shortjournal = {K{\"o}ln Z Soziol},
	title = {Strukturen ungleichen Erfolgs. Winner-take-all-Konzentrationen und ihre sozialen Entstehungskontexte auf flexiblen Arbeitsm{\"a}rkten},
