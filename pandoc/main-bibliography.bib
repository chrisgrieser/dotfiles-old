	journal = {Zeitschrift f{\"u}r Soziologie},
	keywords = {Quantifizierung,Globalisierung,Hochschulranking,Markt,Ranking,Rankings und M{\"a}rkte},
	number = {4},
	pages = {270--288},
	shorttitle = {How {{Rankings Produce Competition}}},
	title = {How {{Rankings Produce Competition}}: {{The Case}} of {{Global University Rankings}}},
	url = {https://www.degruyter.com/view/j/zfsoz.2018.47.issue-4/zfsoz-2018-0118/zfsoz-2018-0118.xml},
	urlyear = {2018},
	volume = {47},
	year = {2018},
	abstract = {Der Zusammenhang zwischen Rankings und Konkurrenz wird h{\"a}ufig unterstellt, aber selten genauer untersucht. Der vorliegende Aufsatz geht ihm am Beispiel globaler Universit{\"a}tsrankings nach. Ausgehend von einem soziologischen Verst{\"a}ndnis von Konkurrenz bestimmen wir â€žRanken`` als eine soziale Operation, die vier Teiloperationen miteinander kombiniert: Vergleich von Leistungen, Quantifizierung, Visualisierung, und wiederholte Publikation. Visualisierung und Publikation stehen f{\"u}r die in der Literatur bisher kaum ber{\"u}cksichtigte performative Dimension von Rankings, die f{\"u}r die Analyse des Zusammenhangs zwischen Rankings und Konkurrenz von zentraler Bedeutung ist. Auf dieser Grundlage zeigen wir, wie globale Universit{\"a}tsrankings zur Konstruktion von Konkurrenz beitragen: durch (a) Globalisierung eines spezifischen Exzellenzdiskurses; (b) Verknappung von Reputation; (c) Transformation einer stabilen in eine dynamische Statusordnung. Wir schlie{\ss}en mit einer Diskussion von Implikationen dieser Analyse f{\"u}r die soziologische Erforschung von Konkurrenz und ihrer gesellschaftlichen Effekte.},
	bdsk-url-1 = {https://www.degruyter.com/view/j/zfsoz.2018.47.issue-4/zfsoz-2018-0118/zfsoz-2018-0118.xml},
	bdsk-url-2 = {https://doi.org/10.1515/zfsoz-2018-0118}}

