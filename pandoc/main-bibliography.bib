@article{Hirschauer1999,
	author = {Hirschauer, Stefan},
	journal = {Soziale Welt: Zeitschrift f{\"u}r sozialwissenschaftliche Forschung und Praxis},
	keywords = {Architektursoziologie,Fahrstuhl,Interaktion,Mobilit{\"a}t,Rational Choice,Raum,Soziologie-Einf{\"u}rung,Symbolischer Interaktionismus,Techniksoziologie,Temporalit{\"a}t},
	number = {3},
	pages = {221--246},
	title = {Die {{Praxis}} Der {{Fremdheit}} Und Die {{Minimierung}} von {{Anwesenheit}}: Eine {{Fahrstuhlfahrt}}},
	url = {http://www.gesis.org/sowiport/search/id/iz-solis-90255289},
	volume = {50},
	year = {1999},
	abstract = {Der Aufsatz widmet sich der soziologisch bisher unentdeckten, allt{\"a}glichen Praxis des Fahrstuhlfahrens. Obwohl dieses Bef{\"o}rderungsmittel mit r{\"a}umlicher Enge, Fremdheit und der Bildung von Zwangsgemeinschaften in einem asozialen Raum verbunden wird und durch das scheinbare 'Nichtstun' der Menschen kein empirisches Forschungsinteresse weckt, geht der Autor davon aus, dass das Fahrstuhlfahren eine kulturelle 'Interaktionsordnung' im Verh{\"a}ltnis zur Technik offenbart. Seine ethnomethodologische Fallstudie m{\"o}chte zeigen, wie 'die Handlungstr{\"a}gerschaft partiell von Personen auf zivilisierte K{\"o}rper und auf technische Artefakte {\"u}bergeht, deren Z{\"u}ge sich in heterogenen Beitr{\"a}gen zur Praxis des Fahrstuhlfahrens verschr{\"a}nken'. Der Autor gibt zun{\"a}chst einen historischen R{\"u}ckblick auf die Technikgenese und kulturellen Implikationen des Fahrstuhls, um im weiteren - entlang der Chronologie der Praxis - seine Studie folgenderma{\ss}en aufzubauen: 'Den Fahrstuhl nehmen: Rational Choice?', 'Einsteigen: Reihenpositionen in einem Heteromobil', 'Aufstellen und Platzwahl: Die Stehordnung', 'Blickwechsel: Die visuelle Ordnung', 'Wortwechsel: Kontaktgelegenheiten und -zw{\"a}nge', 'Aussteigen: Kollektive Fiktionen'. Im abschlie{\ss}enden Teil wird 'die Praxis der Fremdheit und die Minimierung von Anwesenheit' aus kultursoziologischer Perspektive betrachtet. (ICI)},
	bdsk-url-1 = {http://www.gesis.org/sowiport/search/id/iz-solis-90255289}}

@article{Hirschauer2001,
	author = {Hirschauer, Stefan},
	journal = {Zeitschrift f{\"u}r Soziologie},
	keywords = {Ethnografie},
	number = {6},
	pages = {429-451-451},
	title = {Ethnografisches {{Schreiben}} Und Die {{Schweigsamkeit}} Des {{Sozialen}}. {{Zu}} Einer {{Methodologie}} Der {{Beschreibung}}},
	url = {http://www.zfs-online.org/index.php/zfs/article/view/1089},
	urlyear = {2019},
	volume = {30},
	year = {2001},
	abstract = {Der Aufsatz behandelt ein zentrales Problem der Methodologie der Ethnografie. Er expliziert die Beschreibung als eine elementare soziologische Kulturtechnik. Im Vordergrund steht dabei, deren zentrales Bezugsproblem herauszuarbeiten: die Versprachlichung der 'schweigsamen' Dimension des Sozialen. Das ethnografische Schreiben wird zun{\"a}chst als ein Dokumentationsverfahren eingef{\"u}hrt, das durch avanciertere Aufzeichnungstechniken der qualitativen Sozialforschung entwertet wurde. Diese setzten einen naturalistischen Standard in Bezug auf die Reifikation und Dekontextualisierung von 'Daten'. Nach einer wissenssoziologischen Relativierung dieses Standards werden jene Bezugsprobleme herausgearbeitet, die alle Verfahren unbearbeitet lassen, die mit Aufzeichnungen an eine primordiale Verbalisierung des Sozialen durch Informanten anschlie{\ss}en: Interviews, Diskurs- und Gespr{\"a}chsanalysen. Es sind die Probleme des Stimmlosen, Stummen, Unaussprechlichen, Vorsprachlichen und Unbeschreiblichen, die das ethnografische Schreiben zuallererst zu l{\"o}sen hat. In ihm wird etwas zur Sprache gebracht, das vorher nicht Sprache war. F{\"u}r diese Aufgabe einer Verschiebung der Artikulationsgrenze muss sich die Beschreibung von der Logik der Aufzeichnung abwenden und zu einer theorieorientierten Forschungspraxis werden, die nicht nach ihrer Dokumentationsleistung, sondern nach ihren analytischen Leistungen zu bewerten ist.},
	bdsk-url-1 = {http://www.zfs-online.org/index.php/zfs/article/view/1089}}

@article{Hirschauer2003,
	author = {Hirschauer, Stefan},
	eprint = {40878437},
	eprinttype = {jstor},
	journal = {Soziale Welt},
	keywords = {Einf{\"u}hrung,Gender},
	number = {4},
	pages = {461--482},
	shortjournal = {Soziale Welt},
	shorttitle = {Wozu â€š{{Gender Studies}}'?},
	title = {Wozu â€š{{Gender Studies}}'? {{Geschlechtsdifferenzierungsforschung}} Zwischen Politischem {{Populismus}} Und Naturwissenschaftlicher {{Konkurrenz}}},
	volume = {54},
	year = {2003},
	abstract = {For many, "Gender Studies" has become a label for a huge field, comprising women's studies, men's studies and all investigations into their differences and relations. Understood in that way 'gender studies' would be the name of any science that regards human beings under the aspect of their sex. The article pleads for a more specific notion of gender studies as an enter prise investigating the distinction between the sexes instead of using the distinction for research (like women's and men's studies do). Gender studies in this sense get its contour through dealing with three 'margins' of gender: 1. those gender trouble makers which gave rise to the category of 'gender' in clinical research in the 60s, and who are still vital resources for theoretical innovation in the humanities; 2. the phenomenon of gender indifference which has to be taken seriously both as feature of modern societies and as a clue to understanding sexual discrimination as an unexpected re-enactment of gender; 3. the contributions of 'sex studies' in the life-sciences which have to be observed by gender studies in a science studies manner, and which have to be taken seriously as intellectual competitors, not opponents. In sum, gender studies should be regarded as 'social studies of sexual difference' aiming at a culturalist decomposition of the reifications 'man' and 'woman'.}}

@article{Hirschauer2004,
