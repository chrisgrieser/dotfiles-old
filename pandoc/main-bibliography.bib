	bdsk-url-2 = {https://doi.org/10.1177/1948550612455931}}

@article{BrandleLengfeld2015,
	author = {Br{\"a}ndle, Tobias and Lengfeld, Holger},
	journal = {Zeitschrift f{\"u}r Soziologie},
	keywords = {Bildung},
	langid = {english},
	number = {6},
	pages = {447--467},
	shorttitle = {Erzielen {{Studierende}} Ohne {{Abitur}} Geringeren {{Studienerfolg}}?},
	title = {Erzielen {{Studierende}} Ohne {{Abitur}} Geringeren {{Studienerfolg}}? {{Befunde}} Einer Quantitativen {{Fallstudie}}},
	url = {http://www.zfs-online.org/index.php/zfs/article/view/3239},
	urlyear = {2015},
	volume = {44},
	year = {2015},
	abstract = {Im Jahr 2009 sind die deutschen Hochschulen f{\"u}r beruflich qualifizierte Bewerber ohne schulische Hochschulzugangsberechtigung ge\&ccedil;ffnet worden. Inwiefern unterscheiden sich diese Studierenden hinsichtlich des Studienerfolgs von traditionellen Studierenden mit Abitur? Auf der Basis von \&Uuml;berlegungen zu Kompetenzunterschieden aufgrund verschiedener Bildungsverl\&auml;ufe, zur kulturellen Schlie\&szlig;ung des akademischen Feldes f{\"u}r Personen mit niedrigerer sozialer Herkunft und zu unterschiedlichen Lebensumst\&auml;nden bilden wir Hypothesen zu Leistungsunterschieden. Diese {\"u}berpr{\"u}fen wir anhand von Pr{\"u}fungsleistungsdaten von bis zu 1.142 Bachelor-Studierenden eines Fachbereichs einer deutschen Universit\&auml;t. Die Analysen zeigen, dass nicht-traditionelle Studierende im Vergleich zu Personen mit Abitur im ersten Studienjahr 8,4 Prozentpunkte weniger Lehrveranstaltungen erfolgreich absolvieren, eine um 19,9 Prozentpunkte geringere Wahrscheinlichkeit aufweisen, das Studium innerhalb von mindestens neun Semestern zu beenden und ein um durchschnittlich 0,15 Notenpunkte schlechteres Endergebnis erzielen.},
	bdsk-url-1 = {http://www.zfs-online.org/index.php/zfs/article/view/3239}}

