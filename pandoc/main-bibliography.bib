	doi = {10.1007/s11577-020-00715-x},
	journal = {KZfSS K{\"o}lner Zeitschrift f{\"u}r Soziologie und Sozialpsychologie},
	keywords = {Deduktion,Grounded Theory,Wissenschaftstheorie},
	langid = {german},
	number = {4},
