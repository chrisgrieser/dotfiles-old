@article{KrisamEtAl2017,
	author = {Krisam, Mathias and von Philipsborn, Peter and Meder, Bj{\"o}rn},
	doi = {10.1055/s-0042-121598},
	journal = {Das Gesundheitswesen},
	keywords = {Gesundheit,Nudging},
	langid = {german},
	number = {02},
	options = {useprefix=true},
	pages = {117--123},
	shortjournal = {Gesundheitswesen},
	shorttitle = {Nudging in der Prim{\"a}rpr{\"a}vention},
	title = {Nudging in der Prim{\"a}rpr{\"a}vention: Eine {\"U}bersicht und Perspektiven f{\"u}r Deutschland},
	url = {http://www.thieme-connect.de/DOI/DOI?10.1055/s-0042-121598},
	urlyear = {2020},
	volume = {79},
	year = {2017},
	abstract = {Nudging as a means of influencing human behaviour has received increasing attention by policy makers, including those in the field of public health. Nudges are generally understood as specific aspects of a choice architecture that make certain behaviours more likely to occur without mandating them through binding rules, and without relying on economic incentives. Following the example of the United States and Great Britain, the German government has established a working group tasked with advising the federal government on the use of nudging and other behavioural interventions in policy making. The working group's inception in February 2015 inspired a lively public debate. While numerous opportunities for the use of nudging in primary prevention and health promotion in Germany exist, the concept has not yet been widely used in practice. We discuss the basic theoretical concepts of nudging, relating the underlying ideas to the terminology used in prevention and health promotion. In addition, we present typologies and practical examples for nudging interventions, and discuss criticisms raised in the academic and public debate. Finally, we discuss implications for research and policy, highlighting how nudging and related approaches can be used to strengthen primary prevention in Germany.},
	bdsk-url-1 = {http://www.thieme-connect.de/DOI/DOI?10.1055/s-0042-121598},
	bdsk-url-2 = {https://doi.org/10.1055/s-0042-121598}}

