	journal = {Strategic Management Journal},
	keywords = {Rechtssoziologie,Vertrag,Vertrauen},
	langid = {english},
	number = {8},
	pages = {707--725},
	title = {Do Formal Contracts and Relational Governance Function as Substitutes or Complements?},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/smj.249},
	urlyear = {2018},
	volume = {23},
	year = {2002},
	abstract = {Relational exchange arrangements supported by trust are commonly viewed as substitutes for complex contracts in interorganizational exchanges. Many argue that formal contracts actually undermine trust and thereby encourage the opportunistic behavior they are designed to discourage. In this paper, we develop and test an alternative perspective: that formal contracts and relational governance function as complements. Using data from a sample of information service exchanges, we find empirical support for this proposition of complementarity. Managers appear to couple their increasingly customized contracts with high levels of relational governance (and vice versa). Moreover, this interdependence underlies their ability to generate improvements in exchange performance. Our results concerning the determinants of these governance choices show their distinct origins, which further augments their complementarity in practice. Copyright {\copyright} 2002 John Wiley \& Sons, Ltd.},
	bdsk-url-1 = {https://onlinelibrary.wiley.com/doi/abs/10.1002/smj.249},
	bdsk-url-2 = {https://doi.org/10.1002/smj.249}}

