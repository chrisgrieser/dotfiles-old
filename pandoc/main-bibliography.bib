@article{Zuboff1985,
	author = {Zuboff, Shoshana},
	date-modified = {2022-02-01 15:32:55 +0100},
	doi = {10.1016/0090-2616(85)90033-6},
	journal = {Organizational Dynamics},
	keywords = {Technik & Innovation,Computer,Maschinen,Medientheorie},
	location = {{Netherlands}},
	number = {2},
	pages = {5--18},
	publisher = {{Elsevier Science}},
	shorttitle = {Automate/Informate},
	title = {Automate/Informate: {{The}} Two Faces of Intelligent Technology},
	volume = {14},
	year = {1985},
	abstract = {Examines the role that information technology (IT) can play in restructuring the workplace and discusses 2 divergent conceptions of information technology---"automate" and "informate"---and their respective implications for the organization of work. It is suggested that by its very nature, IT is characterized by a fundamental duality that has not been fully appreciated: IT can be applied to automate operations, with the aim of replacing human effort and skill with a technology that enables the same processes to be performed at less cost and with more control and continuity. Technology can also be used to create information; when a given application is designed to automate, it simultaneously generates information about the underlying processes through which an organization accomplishes its work, a process termed informate. The extent to which either of IT's 2 capacities is emphasized will play a central role in determining the organizational consequences of technological change. The choice of emphasis is a question of strategy and derives from management's conception of the contribution that this technology can make to the business. The database as an organizational surrogate, mastery at the information interface, the divergence of the automating and informating strategies, and the emergence of a new division of labor are discussed. (PsycINFO Database Record (c) 2019 APA, all rights reserved)},
	bdsk-url-1 = {https://doi.org/10.1016/0090-2616(85)90033-6}}

