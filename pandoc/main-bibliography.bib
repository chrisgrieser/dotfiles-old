@article{ZhuLiu2018,
	author = {Zhu, Feng and Liu, Qihong},
	doi = {10.1002/smj.2932},
	journal = {Strategic Management Journal},
	number = {10},
	pages = {2618--2642},
	shortjournal = {Strategic Management Journal},
	shorttitle = {Competing with Complementors},
	title = {Competing with Complementors: {{An}} Empirical Look at {{Amazon}}.Com},
	url = {https://onlinelibrary.wiley.com/doi/full/10.1002/smj.2932},
	urlyear = {2019},
	volume = {39},
	year = {2018},
	abstract = {Research Summary: Platform owners sometimes enter complementors' product spaces and compete against them. Using data from Amazon.com to study Amazon's entry pattern into third-party sellers' product spaces, we find that Amazon is more likely to target successful product spaces. We also find that Amazon is less likely to enter product spaces that require greater seller efforts to grow, suggesting that complementors' platform-specific investments influence platform owners' entry decisions. While Amazon's entry discourages affected third-party sellers from subsequently pursuing growth on the platform, it increases product demand and reduces shipping costs for consumers. We consider the implications of these findings for complementors in platform-based markets. Managerial Summary: Platform owners can exert considerable influence over their complementors' welfare. Many complementors with successful products are pushed out of markets because platform owners enter their product spaces and compete directly with them. To mitigate such risks, complementors could build their businesses by aggregating nonblockbuster products or focusing on products requiring significant platform-specific investments to grow. They should also develop capabilities in new product discovery so that they could continually bring innovative products to their platforms.},
	bdsk-url-1 = {https://onlinelibrary.wiley.com/doi/full/10.1002/smj.2932},
	bdsk-url-2 = {https://doi.org/10.1002/smj.2932}}

@article{Ziegler1984,
	author = {Ziegler, Rolf},
	date-modified = {2022-02-01 15:33:45 +0100},
	journal = {K{\"o}lner Zeitschrift f{\"u}r Soziologie und Sozialpsychologie},
	keywords = {Methoden & Wissenschaftstheorie,Banken,interorganisationale Netzwerke,Netzwerkanalyse},
	number = {3},
	pages = {585--614},
	title = {Das {{Netz}} Der {{Personen}}- Und {{Kapitalverflechtungen}} Deutscher Und {\"O}sterreichischer {{Wirtschaftsunternehmen}}},
	volume = {36},
	year = {1984},
	abstract = {In diesem Aufsatz werden zwei einander erg{\"a}nzende Betrachtungsweisen sozialer Netzwerke unterschieden, die relationale und die positionale Analyse. In der relationalen Analyse wird das Netz als System von Transaktionen aufgefasst, dessen Struktur den Untersuchungsgegenstand bildet. Dagegen ist dieses Muster der Vernetzung unter positionalem Aspekt irrelevant, da hier die strukturelle {\"A}hnlichkeit verschiedener Akteure bzw. Positionen herausgearbeitet werden soll. Die Idee der strukturellen {\"A}hnlichkeit wird am Beispiel der Theorie sozialer Normgeltung n{\"a}her ausgef{\"u}hrt. Ziel ist dabei vor allem, die Beziehung zwischen individueller Ebene (der sozialen Beziehungen zwischen konkreten Akteuren) und struktureller Ebene (der Relationen von "Bl{\"o}cken" strukturell {\"a}hnlichen Akteuren) zu kl{\"a}ren. Der Autor kommt zu dem Ergebnis, dass die Kriterien der Normgeltung, Sanktionsanwendung (Durkheim Kriterium) und kontrafaktisch stabilisierte Verhaltenserwartungen (Luhman Kriterium), nicht auf der individuellen, sondern auf der Ebene der Positionen und Rollen erf{\"u}llt sein m{\"u}ssen. Die formale Definition der Kriterien mit Hilfe von Inklusionsbeziehungen zwischen Booleschen Matrixprodukten erlaubt die Anwendung auf prinzipiell beliebig viele Rollen und Relationsmuster. Anhand von Beispieln wird die Funktion der Blockmodellanalyse als heuristisches Mittel und Pr{\"u}fkriterium verdeutlicht.}}

