@article{Munch-Jurisic2020,
	author = {Munch-Jurisic, Ditte Marie},
	doi = {10.1332/204378920X15844659544839},
	journal = {Global Discourse},
	keywords = {Affekt,Diskriminierung,Emotion,Homophilie,kognitive Dissonanz,Werturteilsfreiheit},
	langid = {english},
	shortjournal = {Global Discourse},
	shorttitle = {Against Comfort},
	title = {Against Comfort: Political Implications of Evading Discomfort},
	url = {https://www.ingentaconnect.com/content/10.1332/204378920X15844659544839},
	urlyear = {2020},
	year = {2020},
	abstract = {We typically think of emotional states as highly individualised and subjective. But visceral gut feelings like discomfort can be better understood as collective and public, when they reflect implicit biases that an individual has internalised. Most of us evade discomfort in favour of comfort, often unconsciously. This inclination, innocent in most cases, also has social and political consequences. Research has established that it is easier to interact with people who resemble us and that such in-group favouritism contributes to subtle forms of discrimination. If we want a more equal and unbiased society, we have a duty to expose ourselves to more discomfort. Living up to this duty requires an enhanced emotional vocabulary that captures the political dimensions of physiological affect. I argue that a better understanding of what I call               interaction discomfort               can mitigate subtle forms of discrimination.{$<$}br /{$><$}br /{$>$}               Key messages               {$<$}br /{$><$}ul{$><$}li{$>$}Political change relies on the mitigation of implicit, affective biases, not just the barring of explicit discrimination.{$<$}/li{$><$}br /{$><$}li{$>$}We are drawn to people in whose company we feel comfortable and avoid situations and people that make us uncomfortable.{$<$}/li{$><$}br /{$><$}li{$>$}To counteract such biases and enhance social mobility, we have a duty to choose more               interaction discomfort               .{$<$}/li{$><$}br /{$><$}li{$>$}Understanding the political dimensions of our gut feelings will make us better equipped to tackle awkward interactions.{$<$}/li{$><$}/ul{$>$}},
	bdsk-url-1 = {https://www.ingentaconnect.com/content/10.1332/204378920X15844659544839},
	bdsk-url-2 = {https://doi.org/10.1332/204378920X15844659544839}}

