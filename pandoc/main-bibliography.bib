@book{Laloux2015,
	author = {Laloux, Frederic},
	date-modified = {2022-02-01 15:33:05 +0100},
	edition = {1 edition},
	isbn = {978-3-8006-4913-6},
	keywords = {Organisation,Human-Relations-Ansatz,Managementliteratur,Organisationsentwicklung,Organisationsgeschichte,Selbstorganisationstheorie},
	langid = {Deutsch},
