@article{WellmanEtAl2019,
	author = {Wellman, Ned and Applegate, J. M. and Harlow, John and Johnston, Erik W.},
	date-modified = {2022-02-01 15:33:05 +0100},
	doi = {10.5465/amj.2017.1475},
	journal = {Academy of Management Journal},
	keywords = {Organisation,Formalit{\"a}t,Hierarchie},
	number = {4},
	pages = {997--1027},
	shortjournal = {AMJ},
	shorttitle = {Beyond the {{Pyramid}}},
	title = {Beyond the {{Pyramid}}: {{Alternative Formal Hierarchical Structures}} and {{Team Performance}}},
	url = {https://journals.aom.org/doi/abs/10.5465/amj.2017.1475},
	urlyear = {2020},
	volume = {63},
	year = {2019},
	abstract = {Formal hierarchical differentiation is a cornerstone of the organizing process. Prior research has focused primarily on pyramid-shaped formal hierarchies, despite documented limitations of the pyramid structure. We adopt a multi-method approach to consider the utility of alternative hierarchical shapes. First, we identify six ``pure type'' formal hierarchies that teams might employ. Next, we develop three propositions explaining the effects of hierarchy on team members' cognition and behavior. We use the propositions to parameterize an agent-based computational model in which formal hierarchical differentiation influences team performance by creating power imbalances that affect team members' perspective-taking motivation, and by influencing members' social identification with the team. The modeling results reveal how the effects of the six hierarchies are contingent upon task characteristics that influence team members' perspective-taking accuracy (e.g., task variety), and enable us to craft an expanded, team-level theory of the association between formal hierarchical differentiation and team performance. A field study of 68 clinical nursing shifts in five mid-sized hospitals supports a key theoretical prediction. Specifically, we find that a negatively skewed (inverse pyramid--shaped) formal hierarchy enhances team performance relatively to a positively skewed (pyramid-shaped) hierarchy when task variety is high, but not when task variety is low.},
	bdsk-url-1 = {https://journals.aom.org/doi/abs/10.5465/amj.2017.1475},
	bdsk-url-2 = {https://doi.org/10.5465/amj.2017.1475}}

