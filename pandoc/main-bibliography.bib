@article{KeeterEtAl2006,
	author = {Keeter, Scott and Kennedy, Courtney and Dimock, Michael and Best, Jonathan and Craighill, Peyton},
	date-modified = {2022-02-01 15:33:45 +0100},
	doi = {10.1093/poq/nfl035},
