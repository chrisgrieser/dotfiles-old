@article{Keyfitz1975,
	author = {Keyfitz, Nathan},
	date-modified = {2022-02-01 15:33:45 +0100},
	doi = {10.2307/1972224},
	eprint = {1972224},
	eprinttype = {jstor},
	journal = {Population and Development Review},
	keywords = {Methoden & Wissenschaftstheorie,Demografie,Erkenntnistheorie,formale Modelle,Kausalit{\"a}t,Mathematik,Multikausalit{\"a}t,Multikolinearit{\"a}t,Wissenschaftstheorie},
