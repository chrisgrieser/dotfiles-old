	keywords = {Wissenschaft,Atomwaffen,Computer,Computerphysik,Computersimulationen,Forschungstechnologien,Mathematik,Monte Carlo,Physik,Platonismus,Statistik,Stochastik,trading zone,Zufallsstichprobe},
	langid = {english},
