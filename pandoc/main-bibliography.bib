	title = {Indicators of Technological Activities --- Comparing Educational, Patent and {{R}} \& {{D}} Statistics in the Case of {{Sweden}}},
	url = {http://www.sciencedirect.com/science/article/pii/0048733395008551},
	urlyear = {2015},
	volume = {25},
	year = {1996},
	abstract = {This paper explores the use of R \&amp; D, patent and educational data as technology indicators. Based on Swedish data, we compare what these indicators tell us about (a) the distribution of innovative activity between large and small firms and (b) the technological competence of a set of firms. We conclude that R \&amp; D data probably not only overstate the concentration of technological activities to larger firms but also understate the total technological activities by underplaying the role of small manufacturing firms and service sector firms. Moreover, we find that, in some instances, educational data may capture the character and the diversity of firms' technological competence better than patents do. The combined use of educational and patent data should allow us to capture both the fullness of the breadth of the technology base and the precise orientation of the technological efforts of firms.},
	bdsk-url-1 = {http://www.sciencedirect.com/science/article/pii/0048733395008551},
	bdsk-url-2 = {https://doi.org/10.1016/0048-7333(95)00855-1}}

