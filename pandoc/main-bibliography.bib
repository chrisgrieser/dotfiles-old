	journal = {New Media \& Society},
	keywords = {cloud,Daten{\"o}konomie & Datafizierung,Datenportabilit{\"a}t,Digitalisierung,Digitalisierung und Kultur,Eigentum,Identit{\"a}t,small data,storage,Temporalit{\"a}t},
