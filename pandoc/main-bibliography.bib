	pages = {42--59},
	shorttitle = {Technologische {{Innovationen}} Und Sektoraler {{Wandel}}. {{Eingriffstiefe}}, {{Adaptionsf{\"a}higkeit}}, {{Transformationsmuster}}},
	title = {Technologische {{Innovationen}} Und Sektoraler {{Wandel}}. {{Eingriffstiefe}}, {{Adaptionsf{\"a}higkeit}}, {{Transformationsmuster}}: {{Ein}} Analytischer {{Ansatz}}},
	url = {http://www.zfs-online.org/index.php/zfs/article/view/1265},
