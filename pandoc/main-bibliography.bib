@article{MaisonnasseEtAl2020,
	author = {Maisonnasse, Pauline and Guedj, J{\'e}r{\'e}mie and Contreras, Vanessa and Behillil, Sylvie and Solas, Caroline and Marlin, Romain and Naninck, Thibaut and Pizzorno, Andres and Lemaitre, Julien and Gon{\c c}alves, Antonio and Kahlaoui, Nidhal and Terrier, Olivier and Fang, Raphael Ho Tsong and Enouf, Vincent and Dereuddre-Bosquet, Nathalie and Brisebarre, Angela and Touret, Franck and Chapon, Catherine and Hoen, Bruno and Lina, Bruno and Calatrava, Manuel Rosa and van der Werf, Sylvie and de Lamballerie, Xavier and Le Grand, Roger},
	doi = {10.1038/s41586-020-2558-4},
	issue = {7826},
