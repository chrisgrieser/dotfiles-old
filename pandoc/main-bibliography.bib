	doi = {10.1177/0163443720948018},
	journal = {Media, Culture \& Society},
	keywords = {Digitalwirtschaft,Wirtschaft,Arbeitssoziologie,Begriffstheorie,Gig Economy,umbrella term},
	langid = {english},
	pages = {0163443720948018},
