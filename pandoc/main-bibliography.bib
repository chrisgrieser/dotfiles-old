@article{Unternaehrer2021,
	author = {Untern{\"a}hrer, Markus},
	date-modified = {2022-02-01 15:32:50 +0100},
	doi = {10.1007/s11577-021-00753-z},
	journal = {KZfSS K{\"o}lner Zeitschrift f{\"u}r Soziologie und Sozialpsychologie},
	keywords = {Digitalwirtschaft,Algorithmen,Empfehlungsdienst,Personalisierung,Vergleich},
	langid = {german},
