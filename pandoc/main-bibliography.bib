	author = {Heintz, Bettina},
	date-modified = {2022-02-01 15:33:00 +0100},
	journal = {Zeitschrift f{\"u}r Soziologie},
	keywords = {Wissenschaft,Computerbeweis,Interpretative Flexibilit{\"a}t,langer Beweis,Mathematik,Soziologie der Mathematik,symbolisch generalisierte Kommunikationsmedien,Systemtheorie},
	number = {5},
	pages = {339--360},
	title = {"{{In}} Der {{Mathematik}} Ist Ein {{Streit}} Mit {{Sicherheit}} Zu Entscheiden". {{Perspektiven}} Einer {{Soziologie}} Der {{Mathematik}}},
	url = {http://www.zfs-online.org/index.php/zfs/article/view/1060},
