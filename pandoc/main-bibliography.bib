@article{Katharina2019,
	author = {Scheidgen, Katharina},
	date-modified = {2022-02-01 15:33:05 +0100},
	doi = {10.12759/HSR.44.2019.4.42-74},
	editora = {GESIS-Leibniz-Institut F{\"u}r Sozialwissenschaften},
	editoratype = {collaborator},
	journal = {Historical Social Research},
	keywords = {Organisation},
	langid = {english},
	number = {4},
	pages = {42--74},
	publisher = {{GESIS - Leibniz-Institut f{\"u}r Sozialwissenschaften}},
	shorttitle = {Sozialer {{Kontext}} von {{Teamformierung}}},
	title = {Sozialer {{Kontext}} von {{Teamformierung}}: {{Warum}} Verl{\"a}uft Die {{Formierung}} Der {{Gr{\"u}ndungsteams}} in Unabh{\"a}ngigen {{Start}}-{{Ups}} Und Universit{\"a}ren {{Spin}}-{{Offs}} Unterschiedlich?{{Social Contexts}} in {{Team Formation}}: {{Why Do Independent Start}}-{{Ups}} and {{University Spin}}-{{Offs Form Teams Differently}}?},
	url = {https://www.ssoar.info/ssoar/handle/document/65467},
	urlyear = {2021},
	version = {1},
	volume = {44},
	year = {2019},
	abstract = {Although the entrepreneurial team has gained increasing attention as a unit of analysis, we still do not understand much about how these teams form. Previous research has focused either on existing social relationships and their role in the search for potential team members or on criteria for selecting team members. Consequently, we do not yet understand the interplay of search and selection. Another long-neglected aspect that is being increasingly recognized in entrepreneurship research is that the entrepreneurial process is influenced by its social context beyond existing social relationships. This social context is another important factor that has to be considered to properly understand team formation. To analyze how specific characteristics of one particularly relevant social context -- namely, the entrepreneurial field -- impact the search for and selection of team members, I conducted a qualitative, multiple-case study that compares innovative new ventures in Berlin. The study shows that different types of ventures in different phases exhibit different team formation patterns based on their different and changing social contexts. From these patterns, I have derived different team-formation mechanisms and propositions about the conditions under which they apply.},
	bdsk-url-1 = {https://www.ssoar.info/ssoar/handle/document/65467},
	bdsk-url-2 = {https://doi.org/10.12759/HSR.44.2019.4.42-74}}

