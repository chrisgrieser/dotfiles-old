	keywords = {Alter,Differenz,Differenzierungstheorie,Ethnie,Gender,Hybridit{\"a}t,Intersektionalit{\"a}t,Kategorien & Klassifizierung,Kreuzung sozialer Kreise,Kultursoziologie,Nation,Reifikation,Religion},
	langid = {german},
