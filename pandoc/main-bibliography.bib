@incollection{BaierMassih-Tehrani2016,
	author = {Baier, Christian and Massih-Tehrani, Nilgun},
	booktitle = {Wissen - Organisation - Forschungspraxis: Der Makro-Meso-Mikro-Link in der Wissenschaft},
	date-modified = {2022-02-01 15:33:00 +0100},
