@incollection{Granstrand2004,
	author = {Granstrand, Ove},
	booktitle = {Bringing {{Technology}} and {{Innovation}} into the {{Boardroom}}: {{Strategy}}, {{Innovation}} and {{Competences}} for {{Business Value}}},
	date-modified = {2022-02-01 15:32:50 +0100},
	doi = {10.1057/9780230512771_7},
