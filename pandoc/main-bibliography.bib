@incollection{Trezzini2010,
	author = {Trezzini, Bruno},
	booktitle = {Handbuch Netzwerkforschung},
	editor = {Stegbauer, Christian and H{\"a}u{\ss}ling, Roger},
	isbn = {978-3-531-15808-2 978-3-531-92575-2},
	keywords = {Mikro-Makro-Problem,Netzwerkanalyse,Netzwerke,Theoriel{\"u}cke},
	langid = {german},
	location = {{Wiesbaden}},
	pages = {193--204},
	publisher = {{VS Verlag f{\"u}r Sozialwissenschaften}},
	title = {Netzwerkanalyse, Emergenz und die Mikro-Makro-Problematik},
	url = {http://link.springer.com/chapter/10.1007/978-3-531-92575-2_17},
	urlyear = {2013},
	year = {2010},
	abstract = {Die Mikro-Makro-Problematik, d.h. die Frage, wie individuelles Handeln und soziale Struktur sich gegenseitig bedingen, ist seit jeher fester Bestandteil sozialwissenschaftlicher Selbstverst{\"a}ndigungsversuche und Gegenstand wiederkehrender, mehr oder minder fruchtbarer akademischer Kontroversen. Sie hat in j{\"u}ngster Zeit von zwei Seiten neue Impulse erhalten. Zum einen ist die Mikro-Makro-Wechselwirkung mit R{\"u}ckgriff auf die neueren Emergenzdiskussionen in den Naturwissenschaften und der Philosophie begrifflich spezifiziert worden (Albert 2005; Healy 1998; Heintz 2004; Renn 2008; Sawyer 2005). Zum anderen wird die netzwerkanalytische Perspektive in zunehmendem Ma{\ss}e als ein Ansatz wahrgenommen, der besonders gut dazu geeignet erscheint, die Mikro- und Makroebene einer theoretischen und methodologischen Integration n{\"a}herzubringen (Haines 1988; Jansen 2003; Trezzini 1998; Weyer 2000). Die Frage nach dem Verh{\"a}ltnis von Mikro- und Makroebene in der Netzwerkforschung kann dementsprechend in zwei Richtungen zugespitzt werden, n{\"a}mlich mit Blick auf eine allgemein sozialwissenschaftliche und mit Blick auf eine spezifisch netzwerkanalytische Fragestellung: Was kann die netzwerkanalytische Perspektive zum besseren empirischen und theoretischen Verst{\"a}ndnis der Mikro-Makro-Interdependenz in sozialwissenschaftlichen Fragestellungen beitragen? Stellen Netzwerke als relationale soziale Gebilde ein eigenst{\"a}ndiges, gar emergentes Analyseniveau dar, und bilden sie dar{\"u}ber hinaus ein im Mesobereich angesiedeltes, privilegiertes Vermittlungsglied zwischen der Mikro- und Makroebene? {\"U}ber welche speziellen Konzepte und Analyseverfahren verf{\"u}gt der netzwerkanalytische Ansatz, um zwischen Mikro- und Makroeigenschaften empirischer Netzwerke zu unterscheiden? Welchen Stellenwert besitzt die Mikro-Makro-Wechselwirkung in netzwerkanalytischen Studien, und wie wird sie konzeptualisiert? Inwiefern kann davon gesprochen werden, dass Netzwerke emergente Charakteristiken besitzen, und wie k{\"o}nnen diese erfasst werden?},
	bdsk-url-1 = {http://link.springer.com/chapter/10.1007/978-3-531-92575-2_17}}

