@incollection{DogruelKatzenbach2018,
	author = {Dogruel, Leyla and Katzenbach, Christian},
	booktitle = {Handbuch Online-Kommunikation},
	date-modified = {2022-02-01 15:32:50 +0100},
	doi = {10.1007/978-3-658-18017-1_6-1},
	editor = {Schweiger, Wolfgang and Beck, Klaus},
