	doi = {10.31219/osf.io/zp4wx},
	keywords = {Algorithmen,Empfehlungsdienst,Musikindustrie,Streaming,Verteilte Kognition},
	shorttitle = {"{{First}} Week Is Editorial, Second Week Is Algorithmic"},
