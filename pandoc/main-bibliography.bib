@article{SchnaudtWeinhardt2017,
	author = {Schnaudt, Christian and Weinhardt, Michael},
	journal = {Informationsdienst Soziale Indikatoren},
	keywords = {Europa,Fl{\"u}chtlinge,Migration},
	langid = {german},
	number = {57},
