	sed -i '' 's/{cursor}/\$\|\$/g' "$file"\
	sed -i '' 's/\\U0001F3DD/ðƒ¿ƒ­ƒ¯/g' "$file"\
	sed -i '' 's/\\U0001F34F/ðƒ¿ƒ°ƒº/g' "$file"\
	sed -i '' 's/\\U0001F3DD/ðƒ¿ƒ¯ƒ½/g' "$file"\
\
	temp=$(mktemp)\
	mv -f "$file" "$temp"\
	printf "# https://espanso.org/docs/\n# ------------------------------------------\n\nmatches:\n" > "$file"\
	cat "$temp" >> "$file"\
\
done\
\

