: 1652719125:0;if which alacritty &> /dev/null ; then\
	function man () {\
	\
	which "$1" &> /dev/null\
	if [[ $? == 1 ]] ; then\
	echo "Command '$1' not installed."\
	return 1\
	fi\
	which "$PAGER" &> /dev/null\
	if [[ $? == 1 ]] ; then\
	echo "Pager '$PAGER' not installed."\
	return 1\
	fi\
\
	\
	(alacritty --option=window.decorations=full --title="man $1" --command man "$1" &)\
	}\
fi
