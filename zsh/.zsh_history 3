function r (){\
	INPUT="$*"\
	if [[ -d "$INPUT" ]]; then\
	cd "$INPUT" || return\
	else\
	SELECTED=$(dirs -l -p | fzf \\
		           -0 -1 \\
		           --query "$INPUT" \\
		           --preview "exa -T -L2 {}" \\
		           --preview-window=right:35% \\
		           --height=80% \\
		           --layout=reverse \\
		           --info=inline\
		           )\
	[[ -z "$SELECTED" ]] && return 130\
\
	cd "$SELECTED" || return\
	fi\
	exa\
}\

