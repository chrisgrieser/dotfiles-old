{
	"title": "fn launcher",
	"version": "0.1",
	"maintainers": ["pseudometa"],
	"import-url": "karabiner://karabiner/assets/complex_modifications/import?url=https://github.com/chrisgrieser/dotfiles/blob/main/.config/karabiner/assets/complex_modifications/fn-launcher.json",
	"homepage": "https://github.com/chrisgrieser/dotfiles/karabiner",
	"rules": [
		{
			"description": "use fn + {letter} to launch various apps",
			"manipulators": [
				{
					"type": "basic",
					"from": {
						"key_code": "c",
						"modifiers": {"mandatory": ["fn"] }
					},
					"to": [{"shell_command": "open -a 'BusyCal'"} ]
				},
				{
					"type": "basic",
					"from": {
						"key_code": "o",
						"modifiers": {"mandatory": ["fn"] }
					},
					"to": [{"shell_command": "open -a 'Obsidian'"} ]
				},
				{
					"type": "basic",
					"from": {
						"key_code": "d",
						"modifiers": {"mandatory": ["fn"] }
					},
					"to": [{"shell_command": "open -a 'Discord'"} ]
				},
				{
					"type": "basic",
					"from": {
						"key_code": "y",
						"modifiers": {"mandatory": ["fn"] }
					},
					"to": [{"shell_command": "open -a 'YouTube'"} ]
				},
				{
					"type": "basic",
					"from": {
						"key_code": "e",
						"modifiers": {"mandatory": ["fn"] }
					},
					"to": [{"shell_command": "open -a 'Sublime Text'"} ]
				},
				{
					"type": "basic",
					"from": {
						"key_code": "r",
						"modifiers": {"mandatory": ["fn"] }
					},
					"to": [{"shell_command": "open -a 'Drafts'"} ]
				},
				{
					"type": "basic",
					"from": {
						"key_code": "s",
						"modifiers": {"mandatory": ["fn"] }
					},
					"to": [{"shell_command": "open -a 'Slack'"} ]
				},
				{
					"type": "basic",
					"from": {
						"key_code": "m",
						"modifiers": {"mandatory": ["fn"] }
					},
					"to": [{"shell_command": "open -a 'Mimestream'"} ]
				},
				{
					"type": "basic",
					"from": {
						"key_code": "b",
						"modifiers": {"mandatory": ["fn"] }
					},
					"to": [{"shell_command": "open -a 'Brave Browser'"} ]
				},
				{
					"type": "basic",
					"from": {
						"key_code": "f",
						"modifiers": {"mandatory": ["fn"] }
					},
					"to": [{"shell_command": "open \"$HOME/Library/Mobile Documents/com~apple~CloudDocs/File Hub/\""} ]
				},
				{
					"type": "basic",
					"from": {
						"key_code": "k",
						"modifiers": {"mandatory": ["fn"] }
					},
					"to": [{"shell_command": "open -a 'Karabiner-Elements'"} ]
				},
				{
					"type": "basic",
					"from": {
						"key_code": "k",
						"modifiers": {"mandatory": ["fn", "shift"] }
					},
					"to": [{"shell_command": "open -a 'Karabiner-EventViewer'"} ]
				},
				{
					"type": "basic",
					"from": {
						"key_code": "z",
						"modifiers": {"mandatory": ["fn"] }
					},
					"to": [{"shell_command": "open -a 'YouTube'"} ]
				},
				{
					"type": "basic",
					"from": {
						"key_code": "a",
						"modifiers": {"mandatory": ["fn"] }
					},
					"to": [{"shell_command": "open -a 'Alfred Preferences'"} ]
				},
				{
					"type": "basic",
					"from": {
						"key_code": "y",
						"modifiers": {"mandatory": ["fn"] }
					},
					"to": [{"shell_command": "open -a 'zoom.us'"} ]
				}
			]
		},
		{
			"description": "use fn alone to trigger emoji inserter",
			"manipulators": [
				{
					"type": "basic",
					"from": {
						"key_code": "fn"
					},
					"to": [
						{
							"key_code": "fn",
							"lazy": true
						}
					],
					"to_if_alone": [
						{
							"key_code": "spacebar",
							"modifiers": ["right_command", "right_control"]
						}
					]
				}
			]
		}
	]
}
