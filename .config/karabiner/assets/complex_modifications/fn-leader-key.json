{
	"title": "Fn Leader Key",
	"version": "0.1",
	"maintainers": ["pseudometa"],
	"import-url": "karabiner://karabiner/assets/complex_modifications/import?url=https://github.com/chrisgrieser/dotfiles/blob/main/.config/karabiner/assets/complex_modifications/fn-leader-key.json",
	"homepage": "https://github.com/chrisgrieser/dotfiles/karabiner",
	"rules": [
		{
			"description": "Fn Leader: switch/launch app",
			"manipulators": [
				{
					"type": "basic",
					"description": "Fn Leader Main",
					"from": {"key_code": "fn" },
					"conditions": [
						{
							"type": "variable_unless",
							"name": "LEADER_FN_TRIGGERED",
							"value": 1
						}
					],
					"to": [
						{
							"set_variable": {
								"name": "LEADER_FN_TRIGGERED",
								"value": 1
							}
						},
						{
							"set_notification_message": {
								"id": "fn-leader-notice",
								"text": "Leader active"
							}
						}
					]
				},
				{
					"description": "2x fn: Emoji inserter",
					"type": "basic",
					"from": {"key_code": "fn"},
					"to": [
						{"key_code": "spacebar", "modifiers": ["right_command", "right_control"] },
						{"set_variable": {"name": "LEADER_FN_TRIGGERED", "value": 0}},
						{"set_notification_message": {"id": "fn-leader-notice", "text": ""} }
					],
					"conditions": [
						{
							"type": "variable_if",
							"name": "LEADER_FN_TRIGGERED",
							"value": 1
						},
						{
							"type": "frontmost_application_unless",
							"bundle_identifiers": ["^com\\.runningwithcrayons\\.Alfred$"]
						},
						{
							"type": "device_if",
							"description": "So fn from foot-pedal doesn't trigger emojis",
							"identifiers": [ {"is_pointing_device": false} ]
						}
					]
				},
				{
					"type": "basic",
					"from": {"key_code": "c" },
					"conditions": [{
							"type": "variable_if",
							"name": "LEADER_FN_TRIGGERED",
							"value": 1
					}],
					"to": [
						{"shell_command": "open -a 'BusyCal'"},
						{"set_variable": {"name": "LEADER_FN_TRIGGERED", "value": 0 }},
						{"set_notification_message": {"id": "fn-leader-notice", "text": ""} }
					]
				},
				{
					"type": "basic",
					"from": {"key_code": "o" },
