				<string>function run() {
	path = "{query}";

	encoded_path = encodeURIComponent(path);
	encoded_path = encoded_path.replaceAll ("%2F","/"); //file hooks cant be read without "/"

	var name = path.replace (/(.*)\/$/,"$1"); 
	var name = name.replace (/.*\/(.*?)$/,"$1"); 

	var hook = "[" + name + "](file://" + encoded_path + ")";

	return hook;
}</string>
